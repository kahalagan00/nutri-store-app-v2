{
  "name": "nutri-store-app-v2-backend",
  "version": "1.0.0",
  "description": "Packages for the Back-End portion of Jhuv Nutri Store V2",
  "main": "app.js",
  "scripts": {
    "start": "node dist/server.js",
    "build": "tsc && parcel build ./src/public/js/index.ts --out-dir ./dist/public/js --out-file bundle.js",
    "start:dev": "ts-node-dev --respawn --transpile-only ./server.ts",
    "start:prod": "NODE_ENV=production node dist/server.js",
    "debug": "node --inspect dist/server.js",
    "watch:js": "parcel watch ./src/public/js/index.ts --out-dir ./dist/public/js --out-file bundle.js",
    "type-check": "tsc --noEmit"
  },
  "author": "Joshmar Morales",
  "license": "ISC",
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/hpp": "^0.2.6",
    "@types/mongoose": "^5.11.96",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.9.0",
    "@types/validator": "^13.12.2",
    "ndb": "^1.1.5",
    "parcel-bundler": "^1.12.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "axios": "^1.7.7",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.5",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "eslint": "^9.14.0",
    "express": "^4.21.1",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^7.4.1",
    "helmet": "^8.0.0",
    "hpp": "^0.2.3",
    "html-to-text": "^9.0.5",
    "install": "^0.13.0",
    "jsonwebtoken": "^9.0.2",
    "leaflet": "^1.9.4",
    "mongodb": "^6.10.0",
    "mongoose": "^8.8.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.16",
    "nodemailer-brevo-transport": "^2.1.0",
    "pug": "^3.0.3",
    "sharp": "^0.33.5",
    "slugify": "^1.6.6",
    "stripe": "^17.3.1",
    "validator": "^13.12.0",
    "xss-clean": "^0.1.4"
  }
}
