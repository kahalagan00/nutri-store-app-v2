{
  "version": 2,
  "builds": [
    {
      "src": "backend/server.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/v1/products(.*)",
      "dest": "backend/server.ts"
    },
    {
      "src": "/api/v1/users(.*)",
      "dest": "backend/server.ts"
    },
    {
      "src": "/api/v1/carts(.*)",
      "dest": "backend/server.ts"
    }
  ]
}
